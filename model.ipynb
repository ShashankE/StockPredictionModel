   "source": [
     "def create_dataset(data, lookback):\n",
       "X, Y = [], []\n",
       "for i in range(lookback, len(data)):\n",
         "X.append(data[i-lookback:i, :])\n",
         "Y.append(data[i, 0:2])\n",
      "return np.array(X), np.array(Y)"
    ]
